{
  "expo": {
    "name": "Balloon Loop",
    "slug": "balloon-loop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "balloonloop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.balloonloop.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Balloon Loop needs your location to show balloon arches near you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF6B9D",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.balloonloop.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FF6B9D",
          "dark": {
            "backgroundColor": "#1F2123"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Balloon Loop to use your location."
        }
      ],
      "expo-apple-authentication"
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
